"use strict";var _DESKTOP_SIZE=1260,isNotAcsbl="ACCESSIBLE"!==window.PAGEDATA.theme,isAcsbl="ACCESSIBLE"===window.PAGEDATA.theme,lastFocused=null;function toggleDiv(divClass){document.querySelector(divClass).classList.toggle("hidden")}function hideDiv(divClass){var divEl=document.querySelector(divClass);divEl.classList.contains("hidden")||divEl.classList.add("hidden")}function showDiv(divClass){document.querySelector(divClass).classList.remove("hidden")}window.showBlockWithCookie=({block:block,control:control,cookieName:cookieName,expires:expires="",value:value="1",path:path="/",hideClass:hideClass="hidden"})=>{if(block&&!new RegExp(cookieName).test(document.cookie)){block.classList.remove("hidden");const[domainZone,domainName=""]=window.location.hostname.split(".").reverse(),domainTail=[domainName,domainZone].join("."),clickHandler=()=>{setCookie(cookieName,value,expires,path,domainTail,!0),block.classList.add(hideClass),control.removeEventListener("click",clickHandler)};control.addEventListener("click",clickHandler)}},window.addEventListener("DOMContentLoaded",(function(){if("undefined"!=typeof authClass){var authObj=new authClass;authObj.init(),window.openAuth=authObj.openAuthForm.bind(authObj)}else window.openAuth=()=>null;var menu=document.querySelectorAll(".j-menu-avalible-item");for(var key in menu)if(menu[key]instanceof HTMLElement){var Item=menu[key];(new dropDownMenu).init(Item)}document.querySelector(".j-rating")instanceof HTMLElement&&(new ratingClass).init();var localeSelect=document.querySelector(".j-icon-locale"),localeSelectLang=document.querySelector(".j-locale-select");localeSelect&&(localeSelect.onclick=function(){localeSelectLang.classList.contains("hidden")?(showDiv(".j-locale-select"),hideDiv(".j-locale-select-open"),showDiv(".j-locale-select-close"),document.querySelector("li:first-child a").focus()):(hideDiv(".j-locale-select"),showDiv(".j-locale-select-open"),hideDiv(".j-locale-select-close"))});var searchOpen=document.querySelector(".j-sear