function groupHeaderMenu(){this.container=null,this.btn=null,this.list=null}document.addEventListener("DOMContentLoaded",(function(){groupHeaderMenu.createHandlers()})),groupHeaderMenu.prototype.init=function(element){if(this.container=element,this.btn=getDirectChild(this.container,"j-group-header-menu__mobile"),this.btn){var self=this;this.list=getDirectChild(this.container,"j-group-header-menu__list"),this.btn.addEventListener("click",(function(){self.toggle(self)})),window.addEventListener("click",(function(event){self.list.contains(event.target)||self.btn.contains(event.target)||self.hide(self)}))}},groupHeaderMenu.prototype.toggle=function(ctx){ctx.btn.className=toggleClass(ctx.btn.className,"active"),ctx.list.className=toggleClass(ctx.list.className,"active")},groupHeaderMenu.prototype.hide=function(ctx){ctx.btn.className=hideClass(ctx.btn.className,"active"),ctx.list.className=hideClass(ctx.list.className,"active")},groupHeaderMenu.createHandlers=function(){for(var menuItem,menuBoxes=document.querySelectorAll(".j-group-header-menu"),lx=menuBoxes.length,i=0;i<lx;i++)menuItem=void 0,menuItem=menuBoxes[i],(new groupHeaderMenu).init(menuItem)};